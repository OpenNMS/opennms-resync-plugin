<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
    <!-- TODO: Make these events nice -->
    <event>
        <uei>uei.opennms.org/resync/started</uei>
        <event-label>OpenNMS Resync Plugin: Start resync process</event-label>
<!--        <descr>Successfully sent event via REST API for alarm with reduction key: %parm[reductionKey]%</descr>-->
<!--        <logmsg dest="logndisplay">OpenNMS Resync Plugin: Successfully sent event for alarm: %parm[reductionKey]%</logmsg>-->
        <descr></descr>
        <logmsg></logmsg>
        <severity>Normal</severity>
    </event>
    <event>
        <uei>uei.opennms.org/resync/finished</uei>
        <event-label>OpenNMS Resync Plugin: Resync process finished</event-label>
<!--        <descr>Failed to sent event via REST API for alarm with reduction key: %parm[reductionKey]%. Message is: %parm[message]%</descr>-->
<!--        <logmsg dest="logndisplay">OpenNMS Resync Plugin: Failed to sent event for alarm: %parm[reductionKey]%</logmsg>-->
        <descr></descr>
        <logmsg></logmsg>
        <severity>Normal</severity>
    </event>
    <event>
        <uei>uei.opennms.org/resync/timeout</uei>
        <event-label>OpenNMS Resync Plugin: Resync process reached timeout</event-label>
<!--        <descr>Failed to sent event via REST API for alarm with reduction key: %parm[reductionKey]%. Message is: %parm[message]%</descr>-->
<!--        <logmsg dest="logndisplay">OpenNMS Resync Plugin: Failed to sent event for alarm: %parm[reductionKey]%</logmsg>-->
        <descr></descr>
        <logmsg></logmsg>
        <severity>Warning</severity>
    </event>

    <event>
        <uei>uei.opennms.org/resync/alarm</uei>
        <event-label>OpenNMS Resync Plugin: Resync alarm</event-label>
        <!--        <descr>Failed to sent event via REST API for alarm with reduction key: %parm[reductionKey]%. Message is: %parm[message]%</descr>-->
        <!--        <logmsg dest="logndisplay">OpenNMS Resync Plugin: Failed to sent event for alarm: %parm[reductionKey]%</logmsg>-->
        <descr></descr>
        <logmsg></logmsg>
        <severity>Indeterminate</severity>

        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.28458.1.26.3.0.2</mevalue>
            </maskelement>
        </mask>
    </event>
</events>